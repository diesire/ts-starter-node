{
  "name": "@diesire/ts-node-starter",
  "version": "0.0.0",
  "description": "Starter project for node packages with TS, Jest, Eslint, Prettier",
  "main": "dist/main.js",
  "types": "dist/main.d.ts",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:diesire/ts-node-starter.git",
    "directory": "utils/ts-node-starter"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "eslint \"src/**\" --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage"
  },
  "keywords": [],
  "author": "QTeam <qteam.skywise.fr@capgemini.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^25.1.0",
    "@types/node": "^13.5.1",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.6.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1",
    "ts-jest": "^25.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "dependencies": {}
}
